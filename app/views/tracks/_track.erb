<tr id="track_<%= track.id %>" class="album_list_item <%= cycle('row_even', 'row_odd') %>"
  data-track-id="<%= track.id %>"
  data-track-ext="<%= track.file_ext %>"
>
  <td class="track_num">
    <%= track.number %>
  </td>
  <td class="track_name">
    <%= track.name %>
  </td>
  <td class="track_time">
    <%= track.time %>
  </td>
  <td>
    RATING
  </td>
</tr>