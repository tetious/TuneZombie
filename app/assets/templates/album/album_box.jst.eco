<tr>
  <td class="album_box_art">
    <img class="album_art" src="album_art/<%= @album.id %>" />
  </td>
  <td class="album-box-wrapper-cell">
    <table class="album_box">
      <tr>
        <td class="album_box_details">
          <div><%= @album.name %> - <%= @album.artist_name %></div>
        </td>
      </tr>
      <tr>
        <td>
          <span id="album-block-tracks"></span>
        </td>
      </tr>
    </table>
  </td>
</tr>
